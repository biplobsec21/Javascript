function answer(data) {
    
    let bn=0;
    let sbn=0;
    let cmp=0;
    let max=data[0];
 
        
    bn=biggest(data,max);
    data[bn.index]=0;
    sbn=biggest(data,max);
    
    return `biggest: ${bn.max} and second biggest: ${sbn.max}`;
    
}
function biggest(data,val){
    
    let result={
        max: 0,
        index: 0
    };
    for(let cnt=1;cnt<data.length;cnt++){
            if(data[cnt] > val){
                val = data[cnt];
                index=cnt;
            }
    }
    result.max=val;
    result.index=index;
    return result;
}

