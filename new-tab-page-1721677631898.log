--Camel case conversion --

function answer(data) {
    let result="";
    let word="";
    let VfirstLetter=true;
    let NextFirstLetter=true;
    for(let elm of data){
        
        if(elm === " "){
             result=result + word;
             word="";
             NextFirstLetter=true;
        }else{
            if(VfirstLetter){
                elm = elm.toLowerCase();
                VfirstLetter = false;
            }

            if(!VfirstLetter && NextFirstLetter){
                 elm = elm.toUpperCase();
                 NextFirstLetter = false;
            }else{
                elm = elm.toLowerCase();
            }
            word +=elm;
        }
    }
    
     result=result + word;
    return result;
    
}

